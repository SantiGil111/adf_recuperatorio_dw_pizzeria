{
	"name": "df_gold_hechos_pizzas",
	"properties": {
		"folder": {
			"name": "Gold"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_sql_silver_order_details",
						"type": "DatasetReference"
					},
					"name": "SilverOrderDetails"
				},
				{
					"dataset": {
						"referenceName": "ds_sql_gold_orders",
						"type": "DatasetReference"
					},
					"name": "Orders"
				},
				{
					"dataset": {
						"referenceName": "ds_sql_gold_pizzas",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "select1"
				},
				{
					"name": "lookup1"
				},
				{
					"name": "lookup2"
				},
				{
					"name": "select2"
				}
			],
			"scriptLines": [
				"source(output(",
				"          order_details_id as integer,",
				"          order_id as integer,",
				"          pizza_id as string,",
				"          quantity as integer",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> SilverOrderDetails",
				"source(output(",
				"          order_id as integer,",
				"          date as date",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> Orders",
				"source(output(",
				"          pizza_id as string,",
				"          size as string,",
				"          pizza_tipe_id as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> source1",
				"SilverOrderDetails select(mapColumn(",
				"          order_details_id,",
				"          order_id,",
				"          pizza_id,",
				"          quantity",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1",
				"select1, Orders lookup(select1@order_id == Orders@order_id,",
				"     multiple: false,",
				"     pickup: 'any',",
				"     broadcast: 'auto')~> lookup1",
				"lookup1, source1 lookup(select1@pizza_id == source1@pizza_id,",
				"     multiple: false,",
				"     pickup: 'any',",
				"     broadcast: 'auto')~> lookup2",
				"lookup2 select(mapColumn(",
				"          order_details_id,",
				"          order_id = select1@order_id,",
				"          pizza_id = select1@pizza_id,",
				"          quantity,",
				"          order_id = Orders@order_id,",
				"          date,",
				"          pizza_id = source1@pizza_id,",
				"          size",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select2"
			]
		}
	}
}