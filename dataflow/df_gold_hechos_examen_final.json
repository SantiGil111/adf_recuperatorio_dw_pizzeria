{
	"name": "df_gold_hechos_examen_final",
	"properties": {
		"folder": {
			"name": "Gold"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_sql_gold_category",
						"type": "DatasetReference"
					},
					"name": "Category"
				},
				{
					"dataset": {
						"referenceName": "ds_sql_gold_products",
						"type": "DatasetReference"
					},
					"name": "Products"
				},
				{
					"dataset": {
						"referenceName": "ds_sql_gold_sales",
						"type": "DatasetReference"
					},
					"name": "Sales"
				},
				{
					"dataset": {
						"referenceName": "ds_sql_stores",
						"type": "DatasetReference"
					},
					"name": "Stores"
				},
				{
					"dataset": {
						"referenceName": "ds_sql_gold_warranty",
						"type": "DatasetReference"
					},
					"name": "Warranty"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "union1"
				},
				{
					"name": "union2"
				},
				{
					"name": "union3"
				},
				{
					"name": "union4"
				}
			],
			"scriptLines": [
				"source(output(",
				"          category_id as string,",
				"          category_name as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> Category",
				"source(output(",
				"          Product_ID as string,",
				"          Product_Name as string,",
				"          Category_ID as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> Products",
				"source(output(",
				"          sale_id as string,",
				"          sale_date as date,",
				"          store_id as string,",
				"          product_id as string,",
				"          quantity as integer",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> Sales",
				"source(output(",
				"          Store_ID as string,",
				"          Store_Name as string,",
				"          City as string,",
				"          Country as string,",
				"          opened as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> Stores",
				"source(output(",
				"          claim_id as string,",
				"          claim_date as date,",
				"          sale_id as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> Warranty",
				"Category, Products union(byName: true)~> union1",
				"union1, Sales union(byName: true)~> union2",
				"union2, Stores union(byName: true)~> union3",
				"union3, Warranty union(byName: true)~> union4"
			]
		}
	}
}